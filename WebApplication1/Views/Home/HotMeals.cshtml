@{
}


@model IEnumerable<WebApplication1.Entities.HotMeal>



<!DOCTYPE>

<body>
   
    <h1>Hot Meals</h1>
    <div style="margin:5vh">
        @foreach (var item in Model)
        {
            <p>Id: @item.Id</p>
            <p>Name: @item.Name</p>
            <p>Type: @item.Recipe</p>
            <hr />
        }
    </div>
   


</body>
</html>

